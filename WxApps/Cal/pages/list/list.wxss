
.content{
height: 100%;
box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
background-image:url('../assets/images/img/bg.png');
padding: 30rpx 0 30rpx;
}
.item{
    width: 90%;
    min-height:100rpx;
    line-height:100rpx;
    text-shadow: 0 1px 1px rgba(0, 0, 0, .3);
    border-radius: 5px;
    text-align: center;

    margin: 10rpx auto;

    background: #f78d1d;
}